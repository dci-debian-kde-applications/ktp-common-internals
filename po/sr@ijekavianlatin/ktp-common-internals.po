#: error-dictionary.cpp:37
msgctxt "Verbose user visible error string"
msgid ""
"Looks like you are already connected from other location and the server does "
"not allow multiple connections"
msgstr ""
"Izgleda da ste već povezani sa drugog mjesta a server ne dozvoljava "
"višestruke veze"

#: error-dictionary.cpp:39
msgctxt "Verbose user visible error string"
msgid "Authentication of your account failed (is your password correct?)"
msgstr ""
"Autentifikacija vašeg nalog nije uspjela (da li vam je lozinka ispravna?)"

#: error-dictionary.cpp:41
msgctxt "Verbose user visible error string"
msgid ""
"The channel is too busy now to process your request. Try again in a few "
"minutes"
msgstr ""
"Kanal je trenutno prezauzet da bi obradio vaš zahtjev. Pokušajte ponovo za "
"par minuta"

#: error-dictionary.cpp:43
msgctxt "Verbose user visible error string"
msgid "The connection was canceled on your request"
msgstr "Povezivanje je poništeno na vaš zahtjev"

#: error-dictionary.cpp:45
msgctxt "Verbose user visible error string"
msgid "The SSL/TLS certificate received from server is expired"
msgstr "Primljeni SSL/TLS sertifikat sa servera je istekao"

#: error-dictionary.cpp:47
msgctxt "Verbose user visible error string"
msgid ""
"The SSL/TLS certificate received from server has different fingerprint than "
"expected"
msgstr ""
"Primljeni SSL/TLS sertifikat sa servera ima drugačiji otisak nego što je "
"očekivan"

#: error-dictionary.cpp:49
msgctxt "Verbose user visible error string"
msgid ""
"The SSL/TLS certificate received from server has different hostname than "
"expected"
msgstr ""
"Primljeni SSL/TLS sertifikat sa servera ima drugačije ime domaćina nego što "
"je očekivano"

#: error-dictionary.cpp:51
msgctxt "Verbose user visible error string"
msgid "The SSL/TLS certificate received from server uses too weak encryption"
msgstr "Primljeni SSL/TLS sertifikat sa servera koristi preslabo šifrovanje"

#: error-dictionary.cpp:53
msgctxt "Verbose user visible error string"
msgid "The SSL/TLS certificate received from server is invalid"
msgstr "Primljeni SSL/TLS sertifikat sa servera nije valjan"

#: error-dictionary.cpp:55
msgctxt "Verbose user visible error string"
msgid "The SSL/TLS certificate received from server has exceeded length limit"
msgstr "Primljeni SSL/TLS sertifikat sa servera prekoračio je dužinu znakova"

#: error-dictionary.cpp:57
msgctxt "Verbose user visible error string"
msgid "The SSL/TLS certificate received from server has not been activated yet"
msgstr "Primljeni SSL/TLS sertifikat sa servera još uvijek nije aktiviran"

#: error-dictionary.cpp:59
msgctxt "Verbose user visible error string"
msgid "The server did not provide any SSL/TLS certificate"
msgstr "Server nije obezbijedio nijedan SSL/TLS sertifikat"

#: error-dictionary.cpp:61
msgctxt "Verbose user visible error string"
msgid "The SSL/TLS certificate received from server has been revoked"
msgstr "Primljeni SSL/TLS sertifikat sa servera je opozvan"

#: error-dictionary.cpp:63
msgctxt "Verbose user visible error string"
msgid ""
"The SSL/TLS certificate received from server was self-signed by the server "
"and is untrusted"
msgstr ""
"Primljeni SSL/TLS sertifikat sa servera samopotpisan je od strane servera i "
"zato nepouzdan"

#: error-dictionary.cpp:65
msgctxt "Verbose user visible error string"
msgid ""
"The SSL/TLS certificate received from server was not signed by a trusted "
"certificate authority"
msgstr ""
"Primljeni SSL/TLS sertifikat sa servera nije potpisao nijedan pouzdani "
"autoritet za sertifikate"

